help() #to get help for a particular topic
q() #to quit r studio
data() #to see all datasets by default
library() #to see default libraries installed
library(help=MASS) #to get help for a particular package
install.packages("package-name-here") # to install a package

ls() #list objects in the current R session
rm("var name") #removes a variable from current R session
rm(list = ls())  #remove all variables in the current R session

getwd() #get teh current working directory